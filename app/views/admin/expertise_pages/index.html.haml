.page-header
  = link_to "Add new page", new_admin_expertise_page_path, :class => 'btn btn-default pull-right'
  %h2 Our expertise

= comfy_form_for @page, :as => :page, url: admin_expertise_page_path(@page), :html => {:multipart => true} do |form|
  = render form

= render 'comfy/admin/cms/partials/pages_before'

%ul.list
  = render :partial => 'index_branch', :collection => @pages, :editions => @editions

= render 'comfy/admin/cms/partials/pages_after'
